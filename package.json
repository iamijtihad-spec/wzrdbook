{
  "name": "grit-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "portal": "npx tsx scripts/r2-portal.ts",
    "desktop:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "desktop:build": "next build && electron-builder",
    "discord:setup": "npx tsx lib/discord-setup.ts",
    "discord:seed": "npx tsx scripts/discord/seed-channels.ts",
    "bot:grit": "npx tsx scripts/discord/GritBot.ts",
    "bot:deploy": "npx tsx scripts/discord/deploy-commands.ts",
    "pages:build": "npx open-next-build"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.wzrdbook.gritdashboard",
    "productName": "GRIT Dashboard",
    "mac": {
      "category": "public.app-category.music",
      "identity": null
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "electron/**/*",
      ".next/**/*",
      "public/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "asarUnpack": [
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@coral-xyz/anchor": "^0.29.0",
    "@metaplex-foundation/js": "^0.20.1",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@solana-mobile/wallet-adapter-mobile": "^2.2.5",
    "@solana/spl-token": "^0.4.14",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-walletconnect": "^0.1.21",
    "@solana/web3.js": "^1.98.4",
    "@use-gesture/react": "^10.3.1",
    "bs58": "^6.0.0",
    "canvas": "^3.2.0",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.26",
    "howler": "^2.2.4",
    "lucide-react": "^0.555.0",
    "mime-types": "^3.0.2",
    "next": "^15.5.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@capacitor/ios": "^8.0.0",
    "@opennextjs/cloudflare": "^1.14.8",
    "@tailwindcss/postcss": "^4",
    "@types/chai": "^5.2.3",
    "@types/formidable": "^3.4.6",
    "@types/howler": "^2.2.12",
    "@types/mime-types": "^3.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axios": "^1.13.2",
    "babel-plugin-react-compiler": "1.0.0",
    "chai": "^6.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "pino-pretty": "^13.1.3",
    "prompts": "^2.4.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}